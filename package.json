{
  "name": "openskatemap",
  "homepage": "https://cristiklein.github.io/openstakemap/",
  "private": true,
  "version": "0.7.5",
  "scripts": {
    "dev": "concurrently \"npm:dev:*\"",
    "dev:client": "vite --host",
    "dev:server": "ts-node --project tsconfig.server.json server/start.ts",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "test": "vitest",
    "preview": "vite preview",
    "predeploy": "(git diff-files --quiet || (echo 'Please ensure workdir is clean.' && exit 1)) && vitest run && npm run build",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "cors": "^2.8.5",
    "leaflet": "^1.9.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-leaflet": "^5.0.0",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/leaflet": "^1.9.17",
    "@types/lodash.debounce": "^4.0.9",
    "@types/node": "^22.15.3",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.2",
    "eslint": "^9.22.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "express": "^5.1.0",
    "gh-pages": "^6.3.0",
    "globals": "^16.0.0",
    "jsdom": "^26.1.0",
    "supertest": "^7.1.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.26.1",
    "vite": "^6.3.1",
    "vite-plugin-pwa": "^1.0.0",
    "vitest": "^3.1.2"
  }
}
